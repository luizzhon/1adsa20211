Exercicio2 da aula 13.html:


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Exercicio2 da aula 13</title>
</head>
<body>
    
<button onclick="GolRemo()">Gol do Remo</button> 
<button onclick="GolPaysandu()">Gol do Paysandu</button>
<button onclick="EscanteioRemo()">Escanteio para o Remo</button>
<button onclick="EscanteioPaysandu()">Escanteio para o Paysandu</button>
<h3>Placar do jogo</h3>
<span id="span_texto"></span>
</body>
</html>
<script>
    function GolRemo() {
        span_texto.innerHTML +=('<span style=" color: navy;"> Goool do <b>leão</b>!</span> <br>') ;
    }
    function GolPaysandu() {
        span_texto.innerHTML +=('<span style=" color: cyan;"> Goool da <u>Mucura</u>!</span> <br>') ;
    }
    function EscanteioRemo() {
        span_texto.innerHTML +=('Escanteio para o leão <br>') ;
    }
    function EscanteioPaysandu() {
        span_texto.innerHTML +=('Escanteio para a Mucura <br>') ;
    }
</script>





