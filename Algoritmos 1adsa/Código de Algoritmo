    <center>
    <h2>Fundos coloridos</h2>
    <button onclick="Verde()" style="background-color: green;">Verde</button>
    <button onclick="Amarelo()" style="background-color: yellow;">Amarelo</button>
    <button onclick="Azul()" style="background-color: blue;">Azul</button>
    <center>
    
</body>
</html>
<script>

function Verde() {
    alert ("Natureza");
}
function Amarelo() {
    alert ("Sol");
}
function Azul() {
    alert ("Mar");

}
</script>





